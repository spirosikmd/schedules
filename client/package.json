{
  "name": "jenny-schedule-client",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "author": "Spyros Ioakeimidis <spyrosikmd@gmail.com>",
  "homepage": "./",
  "dependencies": {
    "electron-is-dev": "^0.3.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently -k \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build",
    "preelectron-pack": "yarn build"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "electron": "^2.0.3",
    "electron-builder": "^20.16.0",
    "wait-on": "^2.1.0"
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
